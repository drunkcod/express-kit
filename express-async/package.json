{
  "name": "@drunkcod/express-async",
  "type": "module",
  "version": "0.0.18",
  "description": "express helpers for async life.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*",
    "!lib/**/*.spec.*"
  ],
  "exports": {
    ".": {
      "import": {
        "default": "./lib/index.js",
        "types": "./lib/index.d.ts"
      },
      "require": {
        "default": "./lib/cjs/index.js",
        "types": "./lib/cjs/index.d.ts"
      }
    }
  },
  "scripts": {
    "clean": "rimraf lib",
    "compile": "tsc",
    "cjs:compile": "tsc --module commonjs --outdir lib/cjs",
    "cjs:fixup": "echo '{\"type\": \"commonjs\"}' > lib/cjs/package.json",
    "build": "npm-run-all clean -p compile cjs:compile -s cjs:fixup --silent",
    "test": "node --experimental-vm-modules --disable-warning=ExperimentalWarning node_modules/jest/bin/jest.js"
  },
  "keywords": [
    "express",
    "async"
  ],
  "author": "Tobbe Gyllebring",
  "license": "MIT",
  "dependencies": {
    "express": "^4.21.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.27.1",
    "@jest/globals": "^30.2.0",
    "@types/express": "^4.17.23",
    "@types/node": "^24.7.2",
    "jest": "^30.2.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.5",
    "typescript": "^5.9.3"
  }
}
